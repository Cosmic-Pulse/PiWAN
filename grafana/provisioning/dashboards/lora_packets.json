          "measurement": "mqtt_consumer",
          "orderByTime": "ASC",
          "policy": "default",
          "query": "import \"influxdata/influxdb/schema\"\n\ndata = from(bucket: \"piwan\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  // include both measurements in one stream\n  |> filter(fn: (r) =>\n      r._measurement == \"mqtt_consumer\"\n  )\n\ndata\n  |> pivot(\n      rowKey: [\"_time\"],\n      columnKey: [\"_field\"],\n      valueColumn: \"_value\"\n  )\n  |> filter(fn: (r) => exists r.device_id and exists r.payload)\n  |> map(fn: (r) => ({\n      _time:        r._time,\n      _value:       string(v: r._time) \n                   + \" | \" + r.device_id \n                   + \" | \" + r.payload,\n      device_id:    r.device_id,\n      frame_count:  r.frame_count,\n      received_at:  r.received_at,\n      rssi:         r.rssi,\n      snr:          r.snr,\n      payload:      r.payload,\n      decoded_bytes:r.decoded_bytes\n  }))",
          "refId": "C",
          "resultFormat": "time_series",
